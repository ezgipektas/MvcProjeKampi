
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<br />
<button class="btn btn-primary" id="btn1">Buton 1</button>
<button class="btn btn-warning" id="btn2">Buton 2</button>
<button class="btn btn-success" id="btn3">Buton 2</button>
<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje Kampı!");

    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Mvc Proje Kampı!", "Sweet Alert!");

    });
</script>

<script>
    $('#btn3').click(function () {
        swal("Tebrikler!", "Başarılı bir şekilde eklendi!", "success");

    });
</script>

